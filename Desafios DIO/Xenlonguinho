using System;

class DIO
{
    static void Main(string[] args)
    {
        var C = int.Parse(System.Console.ReadLine());
        
        for (int i = 0; i < C; i++) 
        {
            var N = int.Parse(Console.ReadLine());
            
            Console.WriteLine(N - Math.Floor(Math.Sqrt(N)));
        }
    }
}
