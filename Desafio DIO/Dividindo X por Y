using System;

class Desafio 
{
    static void Main() 
    {

        int limit = Int32.Parse(System.Console.ReadLine());

        for (int i = 0; i < limit; i++) 
        {
            string[] line = System.Console.ReadLine().Split(" ");
            double X = double.Parse(line[0]);
            double Y = double.Parse(line[1]);

            if (Y != 0) 
            {
                double resultado = X / Y;
                System.Console.WriteLine(resultado.ToString("N1"));
            } 
            
            else 
            {
                System.Console.WriteLine("divisao impossivel");
            }
        }
    }
}
